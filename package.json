{
  "version": "0.1.8",
  "name": "antd-phone-input",
  "description": "Advanced, highly customizable phone input component for Ant Design.",
  "keywords": [
    "ant",
    "antd",
    "react",
    "phone",
    "input",
    "number",
    "design",
    "advanced",
    "component",
    "ant-design",
    "customizable",
    "phone-number"
  ],
  "homepage": "https://github.com/ArtyomVancyan/antd-phone-input",
  "bugs": {
    "url": "https://github.com/ArtyomVancyan/antd-phone-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArtyomVancyan/antd-phone-input"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": {
        "default": "./dist/index.d.ts"
      }
    },
    "./legacy": {
      "import": "./dist/legacy/index.esm.js",
      "require": "./dist/legacy/index.cjs.js",
      "types": {
        "default": "./dist/legacy/index.d.ts"
      }
    },
    "./legacy/style": {
      "default": "./dist/legacy/style.less"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:rollup && cp src/legacy/*.less dist/legacy",
    "build:rollup": "rollup -c --configPlugin @rollup/plugin-typescript",
    "build:clean": "rm -r dist || true",
    "test": "jest --config jestconfig.json"
  },
  "license": "MIT",
  "peerDependencies": {
    "antd": ">=4",
    "react": ">=16"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.28",
    "antd": "npm:antd@^5.3.2",
    "antd4": "npm:antd@^4.24.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react-phone-input-2": "^2.15.1"
  }
}
