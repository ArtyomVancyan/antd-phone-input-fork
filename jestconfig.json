{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "/tests/.*\\.test\\.(tsx?)$",
  "moduleNameMapper": {
    "^antd/es/(.+)$": "antd/lib/$1"
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/examples"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "testEnvironment": "jsdom"
}
